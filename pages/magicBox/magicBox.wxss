
/**
  *子功能部分
**/
.subfunction_container{
  display: flex;
  position: absolute;
  z-index: 2;
  flex-direction: row;
  justify-content: center;
  align-items: center;
  width: 100%;
  height: 250rpx;
  margin-top: 116rpx;
  color: white;
  /* background-color: aqua; */
}
.subfunction{
  display: inline-block;
  width: 25%;
  height: 190rpx;
  margin: 30px 10px;
  border-radius:34rpx ;
  box-shadow: 0px 2px 6px 0px rgba(32, 197, 141, 66);
  text-align: center;
  background-color: #20C58D 66%;
}
.subfunction:active{
  background-color: rgb(15, 78, 15);
  box-shadow: 0px 2px 6px 0px rgb(15, 78, 15);
}
.icon{
  display: block;
  width: 128rpx;
  height: 128rpx;
  margin: 10px auto 0;
  /* background-color: green; */
}
