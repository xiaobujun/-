<!-- 识别结果页面 -->

<view class="result_pages">
  <!-- 显示拍照的图片 -->
  <view class="reuslt_image">
    <image src="{{imgBase64}}"></image>
  </view>

  <!-- 简介 -->
  <view class="result_info_simple">
    <view>识别结果</view>
    <swiper indicator-dots="true">
      <view wx:for="{{results}}" wx:for-item="item">
        <swiper-item>
          <i-card title="{{item.name}}" extra="{{item.score}}">
          </i-card>
        </swiper-item>
      </view>
    </swiper>
  </view>
</view>