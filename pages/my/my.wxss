.my_container{
  background-image: linear-gradient(#61D2A9, #FFF);
}
.head_container{
  display: flex;
  flex-direction: column;
  align-items: center;
  color: #FFF;
}
.avatar_container{
  margin-top: 10%;
}
.van-image--round{
  border: 7rpx solid #368560;
}
.nickName{
  margin: 10px auto;
}
.title{
  width: 140px;
  font-weight: bold;
  margin-left:20rpx ;
}
.van_cell_group{
  border: 1px solid rgba(185, 184, 184,.6);
}
.right{
  text-align: left;
}
.van-cell__value .right{
  text-align: left;
  position: relative;

  z-index: 1;
  left: -200px;
  background-color: rgba(129, 79, 79,.0 );
}
.info_button{
  position: absolute;
  width: 900px;
  height: 40px;
  outline:0 none;
  background-color: rgba(158, 45, 45,.0);
}
.info_button::after{
border: none;
}
