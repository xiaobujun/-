.camera_container{
  width: 100%;
  background-color: antiquewhite;
}
.famera_icon{
  display: block;
  position: absolute;
  left: 0; 
  top: 0; 
  right: 0; 
  bottom: 0; 
  width: 207px;
  height: 131px;
  margin:0 auto;
}
.camera{
  width: 90%;
  height: 250px;
  margin: 65px auto 0;
  text-align: center;
  background-color:#D3F9EA ;
  border-radius:20px;
  border: 1px solid #C0C0C0;
}
text{
  font-weight: bold;
  font-size: 50rpx;
  font-family: '微软雅黑',sans-serif;
}

.icon_container{
  width: 80%;
  height:40%;
  margin: 62px auto 0;
  background-color: white;
  border: 1px solid #C0C0C0;
  border-radius: 25px;
}
i{
  display: block;
  width: 50%;
  height: 80%;
  margin: 0 auto;
  /* background-color: yellow; */
}

.camera_icon{
  width: 110px;
  height: 110px;
}